import java.util.HashMap;

public class MnemonicTable {
	private HashMap<String, ICLine> optab;
	MnemonicTable(){
		optab = new HashMap<>();
		BufferedReader br = new BufferedReader(new FileReader(file));
		
	}
	public ICLine getInstruction(String instruction) {
